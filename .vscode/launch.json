    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cli.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Open Active File",
            "program": "${file}",
            "cwd": "${fileDirname}"
        }
    ]
}